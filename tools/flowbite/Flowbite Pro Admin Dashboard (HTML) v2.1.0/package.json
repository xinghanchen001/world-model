		"source-map-loader": "^3.0.0",
		"style-loader": "^3.3.0",
		"tailwindcss": "^3.4.14",
		"webpack": "^5.58.2",
		"webpack-cli": "^4.9.0",
		"webpack-dev-server": "^4.3.1"
	},
	"prosemirror-model": "1.19.3"
}
